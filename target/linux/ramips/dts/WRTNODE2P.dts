/dts-v1/;

/include/ "mt7628an.dtsi"

/ {
	compatible = "mediatek,wrtnode2p", "mediatek,mt7628an-soc";
	model = "WRTnode2P";

        chosen {
                bootargs = "console=ttyS0,57600";
        };

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "i2c", "gpio", "i2s", "refclk", "perst", "spis", "wdt", "sdmode";
				ralink,function = "gpio";
			};
			ephy_p0 {
				ralink,group = "ephy_p0";
				ralink,function = "gpio";
			};
			ephy_p1 {
				ralink,group = "ephy_p1";
				ralink,function = "gpio";
			};
			ephy_p2 {
				ralink,group = "ephy_p2";
				ralink,function = "gpio";
			};
			ephy_p3 {
				ralink,group = "ephy_p3";
				ralink,function = "gpio";
			};
			ephy_p4 {
				ralink,group = "ephy_p4";
				ralink,function = "gpio";
			};
			wled_an {
				ralink,group = "wled_an";
				ralink,function = "wled_an";
			};
		};
	};

	palmbus@10000000 {
		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q256";
				reg = <0 0>;
				linux,modalias = "m25p80";
				spi-max-frequency = <10000000>;
				m25p,chunked-io = <32>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0x7d0000>;
				};

				partition@900000{
					label = "firmware-backup";
					reg = <0x820000 0x7d0000>;
				};

			};

			spidev@1 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "spidev";
				reg = <1 0>;
				spi-max-frequency = <40000000>;
			};
		};

		uart1@d00 {
			status = "okay";
		};

	};

	ethernet@10100000 {
		mtd-mac-address = <&factory 0x4>;
		ralink,port-map = "llllw";
	};

	gpio-leds {
		compatible = "gpio-leds";
		modpwr {
			label = "modpwr";
			gpios = <&gpio0 4 1>;
		};
		modrst {
			label = "modrst";
			gpios = <&gpio1 4 1>;
		};
		heart {
			label = "heart";
			gpios = <&gpio1 7 1>;
		};
		gpioctrl {
			label = "hame:red:gpioctrl";
			gpios = <&gpio1 9 1>;
		};
		gps {
			label = "gps";
			gpios = <&gpio1 8 1>;
		};
		dialstatus {
			label = "dialstatus";
			gpios = <&gpio1 11 1>;
		};
		regstat {
			label = "regstat";
			gpios = <&gpio0 5 1>;
		};
	};
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		vccin {
			label = "BTN_0";
			gpios = <&gpio1 6 0>;
			linux,code = <0x100>;
		};
	};
};

