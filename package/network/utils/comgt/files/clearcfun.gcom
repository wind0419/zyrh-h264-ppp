opengt
 set com 115200n81
 set comecho off
 set senddelay 0.02
 waitquiet 0.2 0.2
 flash 0.1

:start
 print "Trying to clearing cfun\n"
 send "at+cfun=0^m"
 waitfor 5 "OK","ERR","ERROR"
 if % = 0 goto continue
 if % = 1 goto cfunerr
 if % = 2 goto cfunerr
 
 print "Timeout clearing cfun!\n"
 exit 1

:cfunerr
 print "Error clearing cfun!\n"
 exit 1

:continue
 print "cfun=0 OK\n"
 exit 0
